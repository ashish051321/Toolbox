//this is typescript syntax.

function loadData(url: string) {
        let xhr = new XMLHttpRequest();
        xhr.open("GET", url);
        xhr.send();
        xhr.addEventListener("load", function () 
        {
            if (xhr.status === 200) {
                let data = JSON.parse(xhr.responseText);
            }
            else{
                return("some network / file issue.");
            }
        
       }
}//loadData ends
